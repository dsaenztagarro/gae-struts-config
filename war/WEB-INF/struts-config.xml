<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" 
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
 
<struts-config>
 
	<form-beans>
	   <form-bean name="helloWorldForm" type="com.davlanca.timeline.form.HelloWorldForm" />
	</form-beans>
 
	<action-mappings>
	   <action path="/helloWorld" type="com.davlanca.timeline.action.HelloWorldAction"
		name="helloWorldForm">
		<forward name="success" path="/HelloWorld.jsp" />
 
	   </action>
	</action-mappings>
 
</struts-config>